{
  "manifest_version": 2,
  "name": "cookie sync",
  "version": "0.0.1",
  "description": "",
  "homepage_url": "https://github.com/noscripter/cookie-sync",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "persistent": true,
  "web_accessible_resources": [
    "js/inject.js"
  ],
  "permissions": [
    "http://*/",
    "https://*/",
    "cookies",
    "storage"
  ]
}
